{
  "name": "wxt-vue-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "predev": "node scripts/generateIcon.js",
    "prebuild": "node scripts/generateIcon.js",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "eslint": "eslint",
    "dryrun-win": "wxt submit --dry-run --chrome-zip .output/%npm_package_name%-%npm_package_version%-chrome.zip",
    "submit-win": "wxt submit --chrome-zip .output/%npm_package_name%-%npm_package_version%-chrome.zip",
    "dryrun-unix": "wxt submit --dry-run --chrome-zip .output/${npm_package_name}-${npm_package_version}-chrome.zip",
    "submit-unix": "wxt submit --chrome-zip .output/${npm_package_name}-${npm_package_version}-chrome.zip"
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@iconify-prerendered/vue-pepicons-pop": "^0.28.1722795431",
    "@tailwindcss/postcss": "^4.0.6",
    "@types/chrome": "^0.0.280",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/language-plugin-pug": "^2.2.0",
    "@wxt-dev/module-vue": "^1.0.1",
    "chalk": "^5.4.1",
    "dayjs": "^1.11.13",
    "eslint": "^9.20.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vue-pug": "^0.6.2",
    "fs-extra": "^11.3.0",
    "globals": "^15.15.0",
    "postcss": "^8.5.2",
    "postcss-prefix-selector": "^2.1.0",
    "pug": "^3.0.3",
    "sass": "^1.85.0",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "tailwindcss": "^4.0.6",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.24.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10",
    "wxt": "^0.19.13"
  }
}
